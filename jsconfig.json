{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "baseUrl": ".",
    "jsx": "preserve",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["element-plus/global", "unplugin-icons/types/vue"]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "src/setting.ts"],
  "exclude": ["node_modules", "dist"]
}
